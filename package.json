{
  "name": "items-inc",
  "description": "An item incremental.",
  "version": "0.0.1",
  "author": "Kyle Kemp <kyle@seiyria.com>",
  "homepage": "https://seiyria.com/items-inc",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build --prod --release",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "data:init": "node ./setup/data-init",
    "data:format": "node ./setup/data-format",
    "data:download": "node ./setup/data-download",
    "data:font": "webfont -f itemicons -t scss -d src/assets/fonts src/assets/raw/*.svg",
    "postinstall": "npm run data:init && npm run data:download && npm run data:format && npm run data:font"
  },
  "dependencies": {
    "@angular/common": "4.1.3",
    "@angular/compiler": "4.1.3",
    "@angular/compiler-cli": "4.1.3",
    "@angular/core": "4.1.3",
    "@angular/forms": "4.1.3",
    "@angular/http": "4.1.3",
    "@angular/platform-browser": "4.1.3",
    "@angular/platform-browser-dynamic": "4.1.3",
    "@ionic-native/core": "3.12.1",
    "@ionic-native/splash-screen": "3.12.1",
    "@ionic-native/status-bar": "3.12.1",
    "@ionic/storage": "2.0.1",
    "ionic-angular": "3.6.0",
    "ionicons": "3.0.0",
    "ngx-webstorage": "^1.8.0",
    "rxjs": "5.4.0",
    "sw-toolbox": "3.6.0",
    "uuid": "^3.1.0",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@ionic/app-scripts": "2.1.3",
    "@ionic/cli-plugin-ionic-angular": "1.4.0",
    "download-github-repo": "^0.1.3",
    "ionic": "3.6.0",
    "typescript": "2.3.4",
    "webfont": "^7.1.4"
  }
}
